<% form_tag do %>
	<table>
		<tr>
			<td>
				<img src="http://captchator.com/captcha/image/pizza<%=session.session_id%>" />
			</td>
			<td>
				<%= text_field_tag :captcha %>
			</td>
		</tr>
		<tr>
			<td>
				Type in Exact letters
			</td>
			<td>
				<%= submit_tag "login"%>
			</td>
		</tr>
	</table>
<% end %>
